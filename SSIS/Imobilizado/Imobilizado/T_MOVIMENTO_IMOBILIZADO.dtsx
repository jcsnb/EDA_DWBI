                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">-- Depreciações Periódicas e Partidas de Imobilizado que partilham o mês
SELECT	dp.codigo_empresa,dp.ano,dp.mes,dp.numero_imobilizado,dp.subnumero_imobilizado,
		dp.depreciacao_normal_lancar,dp.depreciacao_especial_lancar, pi.montante_lancado,
		pi.depreciacao_normal_proporcional
FROM stg.dbo.t_depreciacao_periodica dp 
LEFT JOIN stg.dbo.t_partida_imobilizado pi ON dp.numero_imobilizado = pi.numero_imobilizado
										   AND dp.subnumero_imobilizado = pi.subnumero_imobilizado
										   AND dp.ano = pi.ano
										   AND dp.mes = pi.mes
UNION ALL
-- Montantes Lançados com impacto no Valor de Aquisição mas que não coincidem em mês de depreciação periódica
SELECT pi.codigo_empresa,pi.ano,pi.mes,pi.numero_imobilizado,pi.subnumero_imobilizado,
		dp.depreciacao_normal_lancar,dp.depreciacao_especial_lancar, pi.montante_lancado,
		pi.depreciacao_normal_proporcional
FROM stg.dbo.t_partida_imobilizado pi 
LEFT JOIN stg.dbo.t_depreciacao_periodica dp  ON dp.numero_imobilizado = pi.numero_imobilizado
										   AND dp.subnumero_imobilizado = pi.subnumero_imobilizado
										   AND dp.ano = pi.ano
										   AND dp.mes = pi.mes
where dp.mes is null</property>
