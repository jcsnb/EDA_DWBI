                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select	pi.codigo_empresa,	pi.numero_imobilizado,	pi.subnumero_imobilizado,pi.ano,
		month(cast(pi.data_referencia as varchar(8))) as mes,
		sum(pi.montante_lancado) as montante_lancado,
		sum(vppi.depreciacao_normal_proporcional) as depreciacao_normal_proporcional
	from e_partida_imobilizado pi
	left join e_valor_proporcional_partida_imobilizado vppi on pi.codigo_empresa = vppi.codigo_empresa
														and pi.numero_imobilizado = vppi.numero_imobilizado
														and pi.subnumero_imobilizado = vppi.subnumero_imobilizado
														and pi.ano = vppi.ano
														and pi.numero_partida = vppi.numero_partida -- 36631
	group by pi.codigo_empresa,	pi.numero_imobilizado,	pi.subnumero_imobilizado,pi.ano,month(cast(data_referencia as varchar(8)))
	having sum(montante_lancado) &lt;&gt; 0 or sum(vppi.depreciacao_normal_proporcional) &lt;&gt; 0</property>
